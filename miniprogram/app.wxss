/**app.wxss**/


.cover-bg-view {
  height: 600rpx;
  position: absolute;
  width: 100%;
  top: 0;
  left: 0;
}
.img {
  height: 100%;
  width: 100%;
}
.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
} 

button {
  background: initial;
}

button:focus{
  outline: 0;
}

button::after{
  border: none;
}


page {
  background: #f6f6f6;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}